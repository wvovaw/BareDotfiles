export EDITOR="nvim"
export READER="zathura"
export VISUAL="nvim"
export TERMINAL="alacritty"
export BROWSER="chromium"
export VIDEO="mpv"
export IMAGE="sxiv"
export OPENER="xdg-open"
export PAGER="bat"
export MANPAGER=$PAGER
export WM="dwm"
export LOCATION="orenburg"
export _JAVA_AWT_WM_NONREPARENTING=1

export GOROOT=/usr/local/go
export GOPATH=~/Dev/Go
export GOBIN=$GOPATH/bin
export CARGOROOT=~/.cargo
export LBIN=~/.local/bin
export SPICETIFY=/usr/lib/spicetify-cli
export SCRIPTS=~/Scripts

export PATH=$PATH:$SCRIPTS:$LBIN:$CARGOROOT/bin:$GOROOT/bin:$GOPATH/bin:$SPICETIFY

LF_ICONS=$(sed ~/.config/diricons \
            -e '/^[ \t]*#/d'       \
            -e '/^[ \t]*$/d'       \
            -e 's/[ \t]\+/=/g'     \
            -e 's/$/ /')
LF_ICONS=${LF_ICONS//$'\n'/:}
export LF_ICONS
